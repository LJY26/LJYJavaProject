<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
    <servlet>
        <servlet-name>StatusRecognise</servlet-name>
        <servlet-class>com.ljy.web0007.servlet.StatusRecognise</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetMessageList</servlet-name>
        <servlet-class>com.ljy.web0007.servlet.GetMessageList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetMessage</servlet-name>
        <servlet-class>com.ljy.web0007.servlet.GetMessage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommitReply</servlet-name>
        <servlet-class>com.ljy.web0007.servlet.CommitReply</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MsgPublish</servlet-name>
        <servlet-class>com.ljy.web0007.servlet.MsgPublish</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommitCriticism</servlet-name>
        <servlet-class>com.ljy.web0007.servlet.CommitCriticism</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StatusRecognise</servlet-name>
        <url-pattern>/StatusRecogniseAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetMessageList</servlet-name>
        <url-pattern>/GetMessageList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetMessage</servlet-name>
        <url-pattern>/GetMessage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommitReply</servlet-name>
        <url-pattern>/CommitReply</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MsgPublish</servlet-name>
        <url-pattern>/MsgPublish</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommitCriticism</servlet-name>
        <url-pattern>/CommitCriticism</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Connector</servlet-name>
        <servlet-class>
            net.fckeditor.connector.ConnectorServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>Connector</servlet-name>
        <url-pattern>
            /fckeditor/editor/filemanager/connectors/*
        </url-pattern>
    </servlet-mapping>



    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>