<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>
    <package name="com.jsj.Struts0006" extends="struts-default">
        <action name="first">
            <result>/first.jsp</result>
        </action>
        <action name="userAction" class="com.ljy.Struts0006.UsersAction">
            <result name="add">user_add.jsp</result>
            <result name="update">user_update.jsp</result>
        </action>
        <action name="greet" class="com.ljy.Struts0006.GreetAction">
            <result name="success">success.jsp</result>
            <result name="error">error.jsp</result>
        </action>
        <action name="ognl" class="com.ljy.Struts0006.OGNLAction">
            <result name="success">success.jsp</result>
        </action>
    </package>
</struts>